import java.util.Scanner; 
public class SumOfSeries
 { 	public static void main(String[] args) {
 		Scanner sc = new Scanner(System.in);
 	// 1/1+1/2+1/3+……….+1/n.	
 	int num = sc.nextInt();		System.out.println(sum(num));	
 	sc.close();	} 
 	static double sum(int n) {
  double a = 0.0; 
  for (int i = 1; i <= n; i++)
   a= a + 1/i; 
   return a; } }
