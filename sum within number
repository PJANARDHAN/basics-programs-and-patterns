input : 20 21 22 23
output : 2 3 4 5  //2+0= 2, 2+1=3, 2+2=4, 2+3=5 like this output

public class Program
{
    public static void main(String[] args) 
    {
        int[] a={20,21,22,23};
        
        for(int i=0;i<4;i++)
        {
            int sum=0;
            int n=a[i];
            while(n>0)
            {
                int num=n%10;
                sum=sum+num;
                 n=n/10;
            }

           System .out.print (sum+" ");
        }
 
 
    }
}
